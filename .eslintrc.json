{
	"extends": "next/core-web-vitals",
	"rules": {
		"@next/next/no-img-element": "off",
		"jsx-a11y/alt-text": "off",
		"react/display-name": "off",
		"react/jsx-max-props-per-line": [
			1,
			{
				"maximum": 1
			}
		],
		"quotes": [
			2,
			"single",
			{
				"avoidEscape": true
			}
		],
		"jsx-closing-bracket-location": 1
	}
}
